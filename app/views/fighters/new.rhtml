<%= error_messages_for :fighter %>

<% form_for :fighter, @fighter, :url => {:action => 'create'}, :html => {:multipart => true} do |f| %>
    <label>The fighter's name:</label> <%= f.text_field :name %> <br/>
    <label>A photo of the fighter:</label> <%= f.file_field :uploaded_data %>
    <%= submit_tag 'Create' %>
<% end %>